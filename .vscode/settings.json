{
	"C_Cpp.intelliSenseEngineFallback": "Disabled",
	"C_Cpp.default.intelliSenseMode": "gcc-x86",
    "c-cpp-compile-run.cpp-flags": "-Wall -Wextra -std=c++17 -Wno-pragma-once-outside-header",
    "c-cpp-compile-run.c-flags": "-Wall -Wextra -Wno-pragma-once-outside-header",
    "C_Cpp.default.cppStandard": "c++17",
	"C_Cpp.default.cStandard": "c11",
	"C_Cpp.loggingLevel": "Error",
 	"clang.cxxflags": [
			"-std=c++17",
			"-Wno-pragma-once-outside-header",
			"-m32"
		],
		"clang.cflags": [
			"-std=c11",
			"-m32"
		],
		"files.associations": {
			"array": "cpp",
			"atomic": "cpp",
			"bit": "cpp",
			"*.tcc": "cpp",
			"cctype": "cpp",
			"clocale": "cpp",
			"cmath": "cpp",
			"cstdarg": "cpp",
			"cstddef": "cpp",
			"cstdint": "cpp",
			"cstdio": "cpp",
			"cstdlib": "cpp",
			"cwchar": "cpp",
			"cwctype": "cpp",
			"deque": "cpp",
			"unordered_map": "cpp",
			"vector": "cpp",
			"exception": "cpp",
			"algorithm": "cpp",
			"functional": "cpp",
			"iterator": "cpp",
			"memory": "cpp",
			"memory_resource": "cpp",
			"numeric": "cpp",
			"optional": "cpp",
			"random": "cpp",
			"string": "cpp",
			"string_view": "cpp",
			"system_error": "cpp",
			"tuple": "cpp",
			"type_traits": "cpp",
			"utility": "cpp",
			"fstream": "cpp",
			"initializer_list": "cpp",
			"iosfwd": "cpp",
			"istream": "cpp",
			"limits": "cpp",
			"new": "cpp",
			"ostream": "cpp",
			"sstream": "cpp",
			"stdexcept": "cpp",
			"streambuf": "cpp",
			"typeinfo": "cpp"
		}
}
